{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/home/home",
			"style": {
				"navigationBarTitleText": "羊毛农场"
			}
		},
		{
			"path": "pages/mine/mine",
			"style": {
				"navigationBarTitleText": "我的"
			}
		},
		{
			"path": "pages/login/login",
			"style": {
				"navigationBarTitleText": "登录"
			}
		},
		{
			"path": "pages/withdrawal/withdrawal",
			"style": {
				"navigationBarTitleText": "提现"
				// "navigationStyle": "custom"
			}
		}
	],
	"condition": { //模式配置，仅开发期间生效
		"current": 0, //当前激活的模式（list 的索引项）
		"list": [{
			"name": "home", //模式名称
			"path": "pages/home/home", //启动页面，必选
			"query": "interval=4000&autoplay=false" //启动参数，在页面的onLoad函数里面得到。
		}, {
			"name": "mine", //模式名称
			"path": "pages/mine/mine", //启动页面，必选
			"query": "interval=4000&autoplay=false" //启动参数，在页面的onLoad函数里面得到。
		}, {
			"name": "login", //模式名称
			"path": "pages/login/login", //启动页面，必选
			"query": "interval=4000&autoplay=false" //启动参数，在页面的onLoad函数里面得到。
		}, {
			"name": "withdrawal", //模式名称
			"path": "pages/withdrawal/withdrawal", //启动页面，必选
			"query": "interval=4000&autoplay=false" //启动参数，在页面的onLoad函数里面得到。
		}]
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "薅羊毛",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
			"borderStyle": "black",
			"selectedColor": "#3ec247",
			"color": "#444444",
			"height":"60px",
			"list": [
				{
					"pagePath": "pages/home/home",
					"iconPath": "static/images/icon-farm.png",
					"selectedIconPath": "static/images/icon-farm-selected.png",
					"text": "农场"
				},{
					"pagePath": "pages/mine/mine",
					"iconPath": "static/images/icon-mine.png",
					"selectedIconPath": "static/images/icon-mine-selected.png",
					"text": "我的"
				}
			]
		},
	"uniIdRouter": {}
}
